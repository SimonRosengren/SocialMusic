<div class="wallBox">
    <p class="siteLogo">W<strong>all</strong></p>
    <div class="scrollDiv">
        <ul class="wallList" id="newMessages"></ul>
        <ul class="wallList" id="wallList">
            @for (int i = Model.Length - 1; i > 0; i--)
            {
                <li>@Model[i].Message</li>
            }
        </ul>
    </div>
    <form id="message">
        <input type="text" id="input" />
        <br />
        <button onclick="Socialmusic.postWallmessage()" class="logInButton">Post</button>
    </form>
</div>

<script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.0.3.min.js"></script>
<script src="~/scripts/SocialMusic/main.js"></script>
<script src="~/scripts/SocialMusic/wallmessage.js"></script>
