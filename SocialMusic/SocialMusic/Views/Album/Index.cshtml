@model SocialMusic.Models.ArtistAlbumViewModel

<h2>Albums</h2>

<div>
<h2>All Albums</h2>

  </div>
<div>
    
    <ul>
        @foreach (var album in Model.Albums)
        {
            <li>@album.Name - @album.Artist</li>
        }
    </ul>   
    <ul>
        @foreach (var artist in Model.Artists)
        {
            <li>@artist.Name</li>
        }
    </ul>     
    <h3>Search Album</h3> 
    <input type="text" id="albumName" size="5" />
    <input type="button" value="Search" onclick="Socialmusic.albumSearch()" />
    <p id="album" />
</div>
<ul id="album" />

<script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.0.3.min.js"></script>
<script src="~/scripts/SocialMusic/main.js"></script>
<script src="~/scripts/SocialMusic/albumsearch.js"></script>