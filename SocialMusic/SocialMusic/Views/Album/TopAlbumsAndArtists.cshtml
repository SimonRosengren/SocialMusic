@model SocialMusic.Models.ArtistAlbumViewModel

<div>

    <ul>
        @foreach (var album in Model.Albums)
        {
            <li>@album.Name - @album.Artist</li>
        }
    </ul>
    <ul>
        @foreach (var artist in Model.Artists)
        {
            <li>@artist.Name</li>
        }
    </ul>
</div>